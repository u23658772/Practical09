
@{
    ViewBag.Title = "Insert";
}

<h2>Insert</h2>
@using (Html.BeginForm("DoInsert", "Home", FormMethod.Post))
{
    <label>Name</label>
    <br />
    @Html.TextBox("Name", null, new { @class = "form-control", @type = "text" });
    <label>ClubName</label>
    <br />
    //@Html.TextBox("ClubName", null, new { @class = "form-control", @type = "text" });
    <div >
        

        <select name="ClubName" id="ClubName">
            <option value="Blue Sky">
                Blue Sky
            </option>
            <option value="Rotary">
                Rotary
            </option>
            <option value="Red Hat">Red Hat</option>
            <option value="Spicy">
                Spicy
            </option>
        </select>
    </div><br />
    <label>Age</label>
    <br />
    @Html.TextBox("Age", null, new { @class = "form-control", @type = "number" });
    <label>Fee</label>
    <br />
    @Html.TextBox("Fee", null, new { @class = "form-control", @type = "number", });
    <br />



    <button type="submit" class="btn btn-primary">Add</button>
}


